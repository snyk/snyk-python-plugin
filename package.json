{
  "name": "snyk-python-plugin",
  "description": "Snyk CLI Python plugin",
  "homepage": "https://github.com/snyk/snyk-python-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-python-plugin"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build-tests": "tsc -p tsconfig-test.json",
    "format:check": "prettier --check '{lib,test}/**/*.{js,ts}'",
    "format": "prettier --write '{lib,test}/**/*.{js,ts}'",
    "prepare": "npm run build",
    "test": "cross-env TS_NODE_PROJECT=tsconfig-test.json tap --node-arg=-r --node-arg=ts-node/register ./test/*.test.{js,ts} -R spec --timeout=900",
    "lint": "npm run build-tests && npm run format:check && eslint --cache '{lib,test}/**/*.{js,ts}'",
    "semantic-release": "semantic-release"
  },
  "author": "snyk.io",
  "license": "Apache-2.0",
  "dependencies": {
    "@snyk/cli-interface": "^2.0.2",
    "tmp": "^0.1.0"
  },
  "devDependencies": {
    "@snyk/types-tap": "^1.1.0",
    "@types/node": "^6.14.6",
    "@types/tmp": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "prettier": "^1.18.2",
    "sinon": "^7.4.1",
    "tap": "^14.6.1",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}
